     name : ChangeLibBouton
     procedure_id : 1486512087067860031
     type_code : 14
     code : |1+
      PROCEDURE ChangeLibBouton(nNation)
      // Tous les boutons sont rendus invisibles
      // All buttons are made invisible
      Obj est objet cNode dynamique
      Bt est objet cBouton dynamique
      Obj = Menu:m_clPremierNode
      
      SI Obj <> Null ALORS
      	Bt = Obj:m_pcldataNode
      	SI nNation = _FRENCH_ ALORS 
      		{Bt:m_sNomDuChamp,indchamp}..Libellé = Bt:m_sLibelléFR
      	SINON
      		{Bt:m_sNomDuChamp,indchamp}..Libellé = Bt:m_sLibelléUS
      	FIN
      	TANTQUE Obj:m_pclnextNode <> Null
      		Obj = Obj:m_pclnextNode	
      		Bt = Obj:m_pcldataNode
      		SI nNation = _FRENCH_ ALORS 
      			{Bt:m_sNomDuChamp,indchamp}..Libellé = Bt:m_sLibelléFR
      		SINON
      			{Bt:m_sNomDuChamp,indchamp}..Libellé = Bt:m_sLibelléUS
      		FIN
      	FIN
      FIN
      
      
