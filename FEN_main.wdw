      PROCEDURE ChangeNation(Num_Nation)
      SI Num_Nation <> Nation() ALORS
      	Nation(Num_Nation)
      	// Titre de la fenêtre - window title
      	MaFenêtre..Titre = <§$0007§>
      	// Change le texte des libellés - change captions text
      	LIB_LANG..Libellé = <§$0000§>
      	LIB_Titre..Libellé = <§$0001§>
      	// Change le texte central - change main text
      	LIB_TEXTAREA..Libellé = MessTraduit(Val(LIB_TEXTAREA..Note))
      	// Change les libellés des boutons - change buttons captions
      	ChangeLibBouton(Num_Nation)
      FIN
      
