   -
     name : ClickMenu
     internal_properties : 
     procedure_id : 1486512087067794495
     type_code : 14
     code : |1+
      PROCEDURE ClickMenu(num)
      // num = 0 	affiche le menu dans son état courant
      // num = 0 	displays the menu according to the current status
      // num > 0 	modifie l'état du menu ...
      //			et affiche le menu avec le sous-menu ouvert ou fermé selon la valeur de menustatus
      // num > 0 	updates the status and ...
      // 			displays the menu with the num-th submenu open or close according to the new status
      
      // variables locales - local variables
      nActiveNation est entier = Nation
      previouslyActiveMenu est entier
      newActiveMenu est entier
      previouslyActiveMenu = Position(MenuStatus, "1")
      currentPosX est entier
      currentPosY est entier
      tailleMenu est entier = Taille(MenuStatus)
      
      // MenuStatus est modifié
      // MenuStatus is updated
      
      SI num > 0 ALORS
      	SI previouslyActiveMenu = num ALORS
      		// juste une RAZ du menu
      		// just reinit the menu
      		menustatus = Répète("0", tailleMenu)
      	SINON
      		// désactive le menu précédent
      		// deactivates the previous menu
      		SI previouslyActiveMenu > 0 alors
      			MenuStatus = Gauche(MenuStatus, previouslyActiveMenu - 1) + "0"
      		Fin
      		MenuStatus += Droite(MenuStatus, tailleMenu - previouslyActiveMenu)
      		// activate le nouveau menu
      		// activates the new menu
      		MenuStatus = Gauche(MenuStatus, num - 1) + "1"
      		MenuStatus += Droite(MenuStatus, tailleMenu - num)
      	FIN
      FIN
      
      // Affiche les boutons en fonction de MenuStatus
      // Display the buttons according to the new status
      
      // Tous les boutons invisibles
      // All buttons are made invisible
      Obj est objet cNode dynamique
      Bt est objet cBouton dynamique
      Obj = Menu:m_clPremierNode
      
      SI Obj <> Null ALORS
      	Bt = Obj:m_pcldataNode
      	{Bt:m_sNomDuChamp,indchamp}..Visible = Faux
      	TANTQUE Obj:m_pclnextNode <> Null
      		Obj = Obj:m_pclnextNode	
      		Bt = Obj:m_pcldataNode
      		{Bt:m_sNomDuChamp,indchamp}..Visible = Faux
      	FIN
      FIN
      
      // Place les boutons et les rend visibles
      // en fonction de MenuStatus
      // Buttons are positionned and made visible
      // according to menustatus
      
      newActiveMenu = Position(MenuStatus, "1")
      
      Obj = Menu:m_clPremierNode
      
      SI Obj <> Null ALORS
      	Bt = Obj:m_pcldataNode
      	// Le premier bouton est toujours visible
      	// The first one is always displayed
      	{Bt:m_sNomDuChamp,indchamp}..Visible = Vrai
      	currentPosX = gninitPosX
      	currentPosY = gninitPosY
      
      	TANTQUE Obj:m_pclnextNode <> Null
      		Obj = Obj:m_pclnextNode	
      		Bt = Obj:m_pcldataNode
      		SI Bt:m_nTypeBouton >= 0 ALORS
      			// Ce bouton est affiche à une nouvelle position
      			// This button is visible with a new position
      			currentPosY += gnincrementY
      			{Bt:m_sNomDuChamp,indchamp}..X = currentPosX
      			{Bt:m_sNomDuChamp,indchamp}..Y = currentPosY
      			{Bt:m_sNomDuChamp,indchamp}..Visible = Vrai
      		SINON
      			SI newActiveMenu > 0 ALORS
      				SI Abs(Bt:m_nTypeBouton) = newActiveMenu ALORS
      					currentPosY += gnincrementY
      					{Bt:m_sNomDuChamp,indchamp}..X = currentPosX
      					{Bt:m_sNomDuChamp,indchamp}..Y = currentPosY
      					{Bt:m_sNomDuChamp,indchamp}..Visible = Vrai
      				FIN
      			FIN
      		FIN
      	FIN // tant que - while
      FIN
